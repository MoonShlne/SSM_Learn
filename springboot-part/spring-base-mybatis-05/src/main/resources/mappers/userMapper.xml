<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myself.mapper.UserMapper">
<select id="findAll" resultType="user">
    select * from user
</select>
    <delete id="delete">
        delete from user where id =#{id}
    </delete>

</mapper>